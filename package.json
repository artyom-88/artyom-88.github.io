{
  "name": "artyom-ganev-site",
  "version": "1.0.0",
  "private": true,
  "author": "Artyom Ganev",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Artyom-Ganev/artyom-ganev-site/issues"
  },
  "homepage": "http://artyom-ganev.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-site.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -o production -d build -t -s **/* !node_modules",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "prettier-check": "prettier -c \"src/**/*.ts?\"",
    "prettier-fix": "prettier -c --write \"src/**/*.ts?\"",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint -p tsconfig.json --fix"
  },
  "dependencies": {
    "axios": "0.19.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-test-renderer": "16.12.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@types/jest": "25.1.2",
    "@types/node": "13.7.1",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/react-redux": "7.1.7",
    "@types/redux-actions": "2.6.1",
    "@types/reselect": "2.2.0",
    "gh-pages": "2.2.0",
    "husky": "4.2.1",
    "lint-staged": "10.0.7",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "react-scripts": "3.3.1",
    "sass": "1.25.0",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.2.0",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint-fix",
      "npm run lint",
      "npm run prettier-fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
