{
  "name": "artem-ganev-site",
  "version": "2.0.0",
  "private": false,
  "author": "Artem Ganev",
  "license": "GPL-3.0",
  "homepage": "https://artyom-88.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/artyom-88/artyom-88.github.io"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "analyze": "pnpm run build --mode analyze",
    "build": "vite build",
    "build:ci": "tsc && pnpm build",
    "build:debug": "vite build --debug",
    "build:dev": "vite build --mode development",
    "depcheck": "pnpm dlx depcheck",
    "deploy:local": "pnpm build:ci && echo $(git rev-parse HEAD)  > ./dist/version && pnpm gh-pages -d dist -u \"ArtÑ‘m <artem_ganev@epam.com>\" -m \"Manual deploy\"",
    "lint": "eslint 'src/**/*.{tsx,ts}' --quiet --max-warnings 1",
    "lint:fix": "eslint 'src/**/*.{tsx,ts}' --fix ",
    "lint:warn": "eslint 'src/**/*.{tsx,ts}'",
    "pre-commit": "tsc && lint-staged",
    "predeploy": "pnpm build",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "start:debug": "vite --debug",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "update": "pnpm dlx npm-check -u -E -s"
  },
  "dependencies": {
    "@ant-design/icons": "5.5.1",
    "@stackoverflow/stacks-icons": "2.27.0",
    "@tanstack/react-query": "5.60.5",
    "antd": "5.22.1",
    "dayjs": "1.11.13",
    "ky": "1.7.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet-async": "2.0.5",
    "react-router-dom": "6.28.0"
  },
  "devDependencies": {
    "@playwright/test": "1.51.0",
    "@tanstack/eslint-plugin-query": "5.60.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-basic-ssl": "1.1.0",
    "@vitejs/plugin-react": "4.3.3",
    "@vitest/coverage-v8": "2.1.5",
    "@vitest/ui": "2.1.5",
    "autoprefixer": "10.4.20",
    "dotenv": "16.4.5",
    "eslint": "8.57.1",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-unicorn": "56.0.0",
    "eslint-plugin-vitest": "0.4.1",
    "gh-pages": "6.2.0",
    "husky": "9.1.6",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.81.0",
    "tailwindcss": "3.4.15",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vite-plugin-svgo": "1.6.2",
    "vitest": "2.1.5"
  },
  "pnpm": {
    "overrides": {
      "micromatch": ">=4.0.8"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write --max-warnings 1"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
