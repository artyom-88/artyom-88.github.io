  "homepage": "http://artyom-ganev.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artyom-Ganev/artyom-ganev-site.git"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -o production -d build -t",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testCI": "CI=true react-scripts test",
    "testCoverage": "react-scripts test --coverage",
    "prettier": "prettier -c --write \"**/*.ts?\"",
    "lint": "eslint --fix \"**/*.ts?\""
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "axios": "0.19.2",
    "moment": "2.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.3",
    "@testing-library/user-event": "12.0.11",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/enzyme-to-json": "1.5.3",
    "@types/jest": "26.0.3",
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-redux": "7.1.9",
    "@types/redux-actions": "2.6.1",
    "@types/redux-saga": "0.10.5",
    "@typescript-eslint/parser": "3.5.0",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.17.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "gh-pages": "3.1.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "react-scripts": "3.4.1",
    "redux-saga-test-plan": "4.0.0-rc.3",
    "sass": "1.26.9",
    "typescript": "3.9.6"
  },
  "resolutions": {
    "**/**/minimist": "^1.2.2",
    "**/**/yargs-parser": "^18.1.2"
  },
  "browserslist": {
    "production": [
      "since 2010"
    ],
    "development": [
      "since 2010"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "actions/**/*.{js,jsx,ts,tsx}",
      "adapter/**/*.{js,jsx,ts,tsx}",
      "api/**/*.{js,jsx,ts,tsx}",
      "components/**/*.{js,jsx,ts,tsx}",
      "container/**/*.{js,jsx,ts,tsx}",
      "model/**/*.{js,jsx,ts,tsx}",
      "reducers/**/*.{js,jsx,ts,tsx}",
      "saga/**/*.{js,jsx,ts,tsx}",
      "selectors/**/*.{js,jsx,ts,tsx}",
      "api/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
